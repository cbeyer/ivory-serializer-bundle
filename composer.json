{
    "name": "ivory/serializer-bundle",
    "type": "symfony-bundle",
    "description": "Serializer for PHP 7.0+ supporting JSON, XML, YAML & CSV",
    "keywords": [ "serializer" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Breu",
            "email": "samuel.breu@bresam.ch",
            "role": "Developer/Maintainer"
        },
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com",
            "role": "Creator/Inactive"
        }
    ],
    "require": {
        "php": "^7.0 || ^8.0",
        "ivory/serializer": "^1.0",
        "symfony/framework-bundle": "^3.0 || ^4.0 || ^5.0 || ^6.0",
        "symfony/cache": "^3.1 || ^4.0 || ^5.0 || ^6.0"
    },
    "require-dev": {
        "doctrine/annotations": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.0",
        "friendsofsymfony/rest-bundle": "^2.0",
        "phpunit/phpunit": "^5.4 || ^6.5 || ^9.5",
        "symfony/form": "^3.0 || ^4.0 || ^5.0",
        "symfony/phpunit-bridge": "^3.0 || ^4.0 || ^5.0",
        "symfony/translation": "^3.0 || ^4.0 || ^5.0",
        "symfony/yaml": "^3.0 || ^4.0 || ^5.0"
    },
    "replace": {
        "egeloen/serializer-bundle": "^1.0.0"
    },
    "autoload": {
        "psr-4": { "Ivory\\SerializerBundle\\": "" }
    }
}
